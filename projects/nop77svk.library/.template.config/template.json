{
	"$schema": "http://json.schemastore.org/template",
	"author": "Peter Hra≈°ko",
	"classifications": [ "NoP77svk", "Library" ],
	"identity": "NoP77svk.Library",
	"name": "Class Library (NoP77svk)",
	"shortName": "nop77svk.lib",
	"sourceName": "{ProjectName}",
	"preferNameDirectory": true,
	"tags": {
		"language": "C#",
		"type": "project"
	},
	"symbols": {
		"Framework": {
			"type": "parameter",
			"description": "Target .NET version for the project",
			"datatype": "choice",
			"defaultValue": "net6.0",
			"replaces": "{NetVersion}",
			"choices": [
				{
					"choice": "net6.0",
					"description": ".NET 6.0"
				},
				{
					"choice": "net5.0",
					"description": ".NET 5.0"
				},
				{
					"choice": "netcoreapp3.1",
					"description": ".NET Core 3.1"
				},
				{
					"choice": "netstandard2.1",
					"description": ".NET Standard 2.1"
				},
				{
					"choice": "netstandard2.0",
					"description": ".NET Standard 2.0"
				}
			]
		},
		"IsNuget": {
			"type": "parameter",
			"description": "Generate NuGet package on build",
			"datatype": "bool",
			"defaultValue": "false",
			"replaces": "{IsNuGet}"
		}
	}
}